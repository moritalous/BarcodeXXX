<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="forest.rice.field.k.barcodexxx" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.zukan.ZukanActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_zukan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.captor.CaptorResultActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_captor" />
        <activity
            android:name=".ui.ranking.RankingActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_ranking" />
        <activity
            android:name=".ui.setting.SettingActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_setting" />
        <activity
            android:name=".ui.webview.WebviewActivity"
            android:label="@string/title_activity_webview" >
        </activity>
        <activity
            android:name=".ui.swap.SwapActivity"
            android:label="@string/title_activity_swap" >
        </activity>
    </application>

</manifest>
